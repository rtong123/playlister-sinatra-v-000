<%= @song.name %>
<h1> Artists <h1>
  <%@songs.each do |song|%>
    <%=song.artist.name%>
      <a href='/artists/<%=song.artist.slug%>'> <%= @song.artist.name %> </a>
      <%song.genres.each do |genre|%>
        <%=genre.name%>
        <a href='/genres/<%=genre.slug%>'> Genre Show Page </a>
  <%end%>
<%end%>
