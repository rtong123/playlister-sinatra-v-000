<h1> Create a new artist</h1>

<form action='/songs' method="POST">

  <label>Name</label>
  <br></br>
  <input type="text" name="song_name" id="Name">


  <%@songs.each do |genre|%>
  <%=binding.pry%>
  <input type="checkbox" name="song[genre_id][]" value="<%=genre.id%>" id="<%=genre.id>"> <%=genre.name%></input>

  <label>Artist Name:</label>
  <br></br>
  <input type="text" name="artist_name" id="Artist Name">

  <input type="submit" value="Create">

</form>
